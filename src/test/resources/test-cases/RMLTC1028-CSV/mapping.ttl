<http://mapping.example.com/rules_000> a void:Dataset.
:source_000 a rml:LogicalSource;
    rdfs:label "locationSource";
    rml:source :csvw_000.
:csvw_000 a csvw:Table;
    csvw:url "./DCs_Locations.csv";
    csvw:dialect :csvw-dialect_000.
:csvw-dialect_000 a csvw:Dialect;
    csvw:delimiter ";".
:source_000 rml:referenceFormulation ql:CSV.
<http://mapping.example.com/rules_000> void:exampleResource :map_Location_000.
:map_Location_000 rml:logicalSource :source_000;
    a rr:TriplesMap;
    rdfs:label "Location".
:s_000 a rr:SubjectMap.
:map_Location_000 rr:subjectMap :s_000.
:s_000 rr:template "http://example.com/Delhaize/Location/{DC}".
:pom_000 a rr:PredicateObjectMap.
:map_Location_000 rr:predicateObjectMap :pom_000.
:pm_000 a rr:PredicateMap.
:pom_000 rr:predicateMap :pm_000.
:pm_000 rr:constant rdf:type.
:pom_000 rr:objectMap :om_000.
:om_000 a rr:ObjectMap;
    rr:constant "https://w3id.org/idlab/ns/ai4fl#Location";
    rr:termType rr:IRI.
:pom_001 a rr:PredicateObjectMap.
:map_Location_000 rr:predicateObjectMap :pom_001.
:pm_001 a rr:PredicateMap.
:pom_001 rr:predicateMap :pm_001.
:pm_001 rr:constant ai4fl:hasIDCode.
:pom_001 rr:objectMap :om_001.
:om_001 a rr:ObjectMap;
    rml:reference "DC";
    rr:termType rr:Literal.
:pom_002 a rr:PredicateObjectMap.
:map_Location_000 rr:predicateObjectMap :pom_002.
:pm_002 a rr:PredicateMap.
:pom_002 rr:predicateMap :pm_002.
:pm_002 rr:constant ai4fl:hasName.
:pom_002 rr:objectMap :om_002.
:om_002 a rr:ObjectMap;
    rml:reference "DC_Name";
    rr:termType rr:Literal.
:pom_003 a rr:PredicateObjectMap.
:map_Location_000 rr:predicateObjectMap :pom_003.
:pm_003 a rr:PredicateMap.
:pom_003 rr:predicateMap :pm_003.
:pm_003 rr:constant ai4fl:hasAddress.
:pom_003 rr:objectMap :om_003.
:om_003 a rr:ObjectMap;
    rml:reference "House no/street";
    rr:termType rr:Literal.
:pom_004 a rr:PredicateObjectMap.
:map_Location_000 rr:predicateObjectMap :pom_004.
:pm_004 a rr:PredicateMap.
:pom_004 rr:predicateMap :pm_004.
:pm_004 rr:constant ai4fl:hasPostalCode.
:pom_004 rr:objectMap :om_004.
